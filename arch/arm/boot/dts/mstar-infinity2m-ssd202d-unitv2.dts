/dts-v1/;
#include "mstar-infinity2m-ssd202d.dtsi"

/ {
	model = "UnitV2";
	compatible = "m5stack,unitv2", "mstar,infinity2m";

	aliases {
		serial0 = &pm_uart;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "console=ttyS0,115200 clk_ignore_unused";
	};

	vcc_dram: regulator@0 {
		compatible = "regulator-fixed";
		regulator-name = "vcc_dram";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-boot-on;
	};
};

&pm_uart {
	status = "okay";
};

&isp {
	status = "okay";
	nand: nand@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-nand";
		reg = <0>;
		spi-max-frequency = <40000000>;
		spi-rx-bus-width = <4>;
	};
};

&usbphy0 {
	status = "okay";
};

&uhc0 {
	status = "okay";
};

//&usbphy1 {
//	status = "okay";
//};

//&uhc1 {
//	status = "okay";
//};

&emac {
	status = "okay";
};

&miu {
	ddr-supply = <&vcc_dram>;
};